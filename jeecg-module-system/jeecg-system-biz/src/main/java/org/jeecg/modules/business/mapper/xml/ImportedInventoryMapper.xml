<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.business.mapper.ImportedInventoryMapper">

    <select id="selectByClientId" parameterType="java.lang.String"
            resultType="org.jeecg.modules.business.entity.ImportedInventory">
        SELECT *
        FROM imported_inventory
        WHERE client_id = #{clientId}    </select>
</mapper>